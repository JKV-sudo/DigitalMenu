{
  "include": ["./src"],
  "exclude": ["./build"],

  "compilerOptions": {
    "outDir": "./build",
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "allowJs": true,
    "checkJs": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  
}
